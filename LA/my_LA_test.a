void main ( ) {
    int64[][] anArr
    int64 sum
 
    anArr <- new Array(4, 5)
    anArr[0][2] <- 5
    anArr[2][4] <- 5
    sum <- add(anArr)

    print(sum)
    return
}

int64 add (int64[][] anArr) {
    int64 result
    int64 temp
    result <- anArr[0][2]   
    temp <- anArr[2][4]
    result <- result + temp
    return result
}
